{
  "name": "SuperSocket.ClientEngine",
  "version": "0.8.0.1",
  "buildOptions": {
    "emitEntryPoint": false,
    "compile": {
      "include": ["../**/*.cs"],
      "exclude": [
          "../SuperSocket.ClientEngine/**/*.cs",
		  "../SuperSocket.ClientEngine.Core/**/*.cs",
          "../Test/**/*.cs",
          "../**/Properties/*.cs",
          "../Properties/*.cs",
          "../**/*.Net20.cs",
          "../**/*.Silverlight.cs",
          "../**/*.Net40.cs",
          "../**/*.Net35.cs",
          "../**/*.WindowsPhone.cs",
          "../Protocol/**/*.cs",
          "../Common/DnsEndPoint.cs"
        ],
      "excludeFiles": [
          "../Common/ConnectAsyncExtension.Net.cs",
          "../Common/GenericDelegates.Func.cs",
          "../Common/GenericDelegates.Action.cs",
          "../Core/SslStream.cs",
          "../Core/NetworkStream.cs"
        ]
    }
  },
  "dependencies": {
      "SuperSocket.ProtoBase": "[1.7.0.10]"
  },
  "frameworks": {
    "netstandard1.3": {
      "buildOptions": {
        "define": [ "NETFX_CORE", "NETSTANDARD", "AWAIT" ]
      },
      "dependencies": {
        "System.Linq": "4.1.0",
        "System.Net.Sockets": "4.1.0",
        "System.Runtime.Extensions": "4.1.0",
        "System.Runtime.InteropServices": "4.1.0",
        "System.Security.Cryptography.X509Certificates": "4.1.0",
        "System.Net.Security": "4.0.0",
        "System.Net.NameResolution": "4.0.0"
      }
    }
  }
}
